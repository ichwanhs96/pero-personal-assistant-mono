{
  "name": "pero-personal-assistant-mono",
  "version": "0.0.1",
  "description": "Pero Ichwan's personal assistant mono repo",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "~27.0.2",
    "@types/node": "^16.11.22",
    "@typescript-eslint/eslint-plugin": "~5.7.0",
    "@typescript-eslint/parser": "~5.7.0",
    "eslint": "~8.4.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~25.3.0",
    "express": "^4.17.2",
    "jest": "~27.4.4",
    "prettier": "~2.5.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.1.1",
    "ts-node": "^10.4.0",
    "tsutils": "~3.21.0",
    "typescript": "~4.5.3"
  },
  "scripts": {
    "start": "nodemon build/src/server.js",
    "start:dev": "nodemon src/server.ts",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "typeorm": "./node_modules/.bin/typeorm -n",
    "migration:create": "./node_modules/.bin/typeorm migration:create -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "./node_modules/.bin/typeorm migration:revert",
    "entity:create": "./node_modules/.bin/typeorm entity:create -n"
  },
  "author": "@Ichwan Haryo Sembodo, template by @Jakub Synowiec",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "~2.3.1",
    "typeorm": "^0.2.41"
  },
  "volta": {
    "node": "16.13.0"
  }
}
